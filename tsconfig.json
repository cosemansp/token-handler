{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "esModuleInterop": true,
    "strict": true,
    "strictNullChecks": true,
    "lib": ["ESNext"],
    "types": ["@types/node"],
    "skipLibCheck": true,
    "noEmit": true,
    "allowImportingTsExtensions": true,
  }
}
